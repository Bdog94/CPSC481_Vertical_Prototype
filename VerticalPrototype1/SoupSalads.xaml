        <Menu x:Name="Allergies" HorizontalAlignment="Left" Height="116" Margin="202,359,0,0" VerticalAlignment="Top" Width="110" Grid.RowSpan="2" Background="#FFEAD2AC" Visibility="Hidden">
            <Button x:Name="GlutenFree" Content="GlutenFree" Height="27" Width="95" Background="#FFEAD2AC" Click="GlutenFree_Click"/>
