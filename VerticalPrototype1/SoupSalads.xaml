            <Button x:Name="AllergySelect" Content="Allergies/Diets" Height="27" Width="95" Background="#FFEAD2AC" Click="AllergySelect_Click"/>
            <Button Content="Button" Height="27" Width="95" Background="#FFEAD2AC"/>
            <Button Content="Button" Height="27" Width="95" Background="#FFEAD2AC"/>
            <Button Content="Button" Height="27" Width="95" Background="#FFEAD2AC"/>
